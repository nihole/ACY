<?xml version="1.0" encoding="UTF-8"?>
<polUni>
	<infraInfra>
		{% for j in interface_range %}
        	{% set action %}{%if j.action%}{{j.action}}{%else%}{{global.action}}{%endif%}{% endset %}
        	{%if not j.action=="ignore"%}
        	{% set name %}{%if j.name%}{{j.name}}{%else%}{{global.name}}{%endif%}{% endset %}
		{% set description %}{%if j.description%}{{j.description}}{%else%}{{global.description}}{%endif%}{% endset %}
		{% set interface_profile %}{%if j.interface_profile%}{{j.interface_profile}}{%else%}{{global.interface_profile}}{%endif%}{% endset %}
                {% set interface_selector %}{%if j.interface_selector%}{{j.interface_selector}}{%else%}{{global.interface_selector}}{%endif%}{% endset %}
	    	<infraAccPortP name="{{interface_profile}}">
			<infraHPortS name="{{interface_selector}}" descr="{{description}}" type="range">
				{% if from_port != "" %}
				<infraPortBlk name="{{name}}" descr="{{description}}" toPort="{{to_port}}" toCard="{{slot_id}}" fromPort="{{from_port}}" fromCard="{{slot_id}}" status="{{action}}"/>
	            {% endif %}		
			</infraHPortS>
		</infraAccPortP>
		{%endif%}{%endfor%}
   </infraInfra>
</polUni>
